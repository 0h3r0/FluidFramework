{
  "build": {
    "content": [
      {
        "files": ["**.md", "**.yml"],
        "exclude": ["old/**", "_site/**", "_template/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**", "examples/**/*.html"]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Prague Documentation",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/Microsoft/Prague",
        "branch": "master"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "_template"],
    "postProcessors": [],
    "markdownEngineName": "dfm-latest",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
