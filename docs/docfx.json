{
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**.yml",
        ],
        "exclude": [
          "_site/**",
          "templates/**",
          "legacy/**",
        ]
      },
      {
        "src": "../",
        "files": [
          "samples/hosts/README.md",
          "packages/drivers/fluid-debugger/README.md",
          ],
        "exclude": ["**/node_modules/**"]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "examples/**/*.html",
          "**.png",
          "**.jpg",
        ]
      },
      {
        "src": "../",
        "files": [
          "packages/drivers/fluid-debugger/images/**.jpg",
        ],
        "exclude": ["**/node_modules/**"]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Fluid Documentation",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/Microsoft/Prague",
        "branch": "master"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/fluid",
    ],
    "postProcessors": [],
    "markdownEngineName": "dfm-latest",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
