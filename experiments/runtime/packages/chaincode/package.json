{
  "name": "@chaincode/sequence",
  "version": "0.1.2",
  "description": "Classic Prague runtime as chaincode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run tsc",
    "build:full": "npm run build && npm run webpack && npm run tslint",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js",
    "tslint": "tslint 'src/**/*.ts'",
    "start": "webpack-serve webpack.config.js --port 8081"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.0.15",
    "webpack-serve": "^2.0.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@prague/api": "^0.1.2",
    "@prague/runtime-definitions": "^0.1.2",
    "@types/node": "^10.5.8"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  }
}
