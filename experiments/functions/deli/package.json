{
  "name": "@prague/deli",
  "version": "0.1.0",
  "description": "Deli ordering service",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run tsc && npm run tslint",
    "build:container": "docker build --build-arg NPM_TOKEN=${NPM_TOKEN} -t deli .",
    "clean": "rimraf dist",
    "docker": "docker run --rm -it -e NPM_TOKEN=$NPM_TOKEN -v $(pwd):/usr/src/server deli /bin/sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "dependencies": {
    "@prague/routerlicious": "^0.1.3858",
    "bytes": "^3.0.0",
    "commander": "^2.19.0",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.11",
    "measured-core": "^1.11.2",
    "moniker": "^0.1.2",
    "node-rdkafka": "^2.4.2",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0",
    "winston": "^3.1.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
