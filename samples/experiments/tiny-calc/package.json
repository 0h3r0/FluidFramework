{
  "name": "@prague/tiny-calc",
  "version": "0.2.0",
  "description": "Tiny Excel-subset expression compiler.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "git clean -fXd -e \\!**/node_modules/**",
    "build": "concurrently npm:tsc npm:tslint",
    "dev": "tsc --project tsconfig.json --watch",
    "bench": "ts-node --project tsconfig.json bench/index.ts",
    "test": "ts-mocha -p tsconfig.json \"test/**/*.{ts,tsx}\"",
    "tsc": "tsc --project tsconfig.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "postinstall": "git submodule init && git submodule update"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "@types/benchmark": "^1.0.31",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "benchmark": "^2.1.4",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2"
  },
  "license": "MIT"
}
