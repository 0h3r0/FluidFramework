{
  "name": "@chaincode/collab-browser",
  "version": "0.0.84",
  "description": "Experimental collaborative browser component",
  "main": "dist/main.bundle.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run webpack && npm run tslint",
    "webpack": "webpack --config webpack.config.js",
    "tslint": "tslint 'src/**/*.ts'",
    "publish-local": "npm publish --registry http://localhost:4873",
    "publish-patch-local": "npm version patch && npm publish --registry http://localhost:4873",
    "update": "npm run build && publish-patch-local",
    "dev": "npm run webpack && npm run tslint -- --fix && npm version patch && npm publish --registry http://localhost:4873 && echo \"http://localhost:3000/loader/x${RANDOM}?chaincode=@chaincode/collab-browser@latest\""
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@types/node": "^10.11.4",
    "clean-webpack-plugin": "^0.1.19",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {},
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "license": "MIT"
}
