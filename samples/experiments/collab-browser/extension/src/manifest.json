{
    "name": "Prague Collaborative Browser",
    "version": "0.1",
    "description": "",
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "webNavigation",
        "system.display",
        "contextMenus"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'",
    "browser_action": {
        "default_title": "Prague Collaborative Browsing",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.bundle.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "exclude_matches": [
                "*://localhost/loader/*"
            ],
            "js": [
                "content.bundle.js"
            ]
        }
    ],
    "manifest_version": 2
}