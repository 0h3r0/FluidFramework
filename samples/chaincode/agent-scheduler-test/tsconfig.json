{
    "extends": "@prague/build-common/ts-common-config.json",
    "exclude": [
        "dist",
        "node_modules"
    ],
    "compilerOptions": {
        "target": "es6",
        "strict": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "sourceMap": true,
        "inlineSources": true
    },
    "include": [
        "src/**/*"
    ]
}
