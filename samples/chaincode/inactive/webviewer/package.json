{
  "name": "@chaincode/webviewer",
  "version": "0.2.0",
  "description": "Classic Prague runtime as chaincode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run tsc && npm run webpack && npm run tslint",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js",
    "tslint": "tslint 'src/**/*.ts'",
    "start": "prague-dev-server --config webpack.config.js --live --package package.json"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@prague/prague-dev-server": "^3.2.5",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@prague/shared-object-common": "^0.1.3542",
    "@prague/map": "^0.1.3542",
    "@prague/runtime-definitions": "^0.1.3542",
    "@prague/shared-string": "^0.1.3542",
    "@prague/stream": "^0.1.3542",
    "@types/node": "^10.5.8"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "license": "MIT"
}
