{
  "name": "apps",
  "version": "0.0.1",
  "description": "Collection of apps built on top of routerlicious",
  "main": "dist/www.js",
  "scripts": {
    "build": "concurrently \"npm run tslint\" \"npm run tsc && npm run webpack\" \"npm run less\"",
    "build:full": "concurrently \"npm run tslint\" \"npm run tsc && npm run webpack:min\" \"npm run less\"",
    "dckr:build": "docker build --build-arg NPM_TOKEN=$(echo $NPM_TOKEN) -t apps .",
    "dckr:start": "docker run -p 7000:3000 apps",
    "webpack": "webpack --config webpack.config.js",
    "webpack:min": "webpack --config webpack.config.js --env.prod",
    "tsc": "tsc",
    "tslint": "tslint 'src/**/*.ts'",
    "less": "lessc src/stylesheets/style.less public/stylesheets/dist/style.css",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prague/routerlicious": "^0.1.2694",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "gitresources": "https://praguenpm.blob.core.windows.net/packages/gitresources-0.2.0.tgz",
    "hjs": "0.0.6",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.1.1",
    "less": "^3.0.4",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "moniker": "^0.1.2",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.12",
    "performance-now": "^2.1.0",
    "rc-slider": "^8.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "request": "^2.86.0",
    "serve-favicon": "^2.4.5",
    "split": "^1.0.1",
    "static-expiry": "0.0.11",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.35",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.0",
    "@types/express-session": "^1.15.6",
    "@types/jquery": "^3.3.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "0.0.37",
    "@types/passport": "^0.4.2",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/request": "^2.47.0",
    "@types/serve-favicon": "^2.2.30",
    "@types/split": "^0.3.28",
    "@types/winston": "^2.3.7",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "source-map-loader": "^0.2.3",
    "speed-measure-webpack-plugin": "^1.2.2",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  }
}
