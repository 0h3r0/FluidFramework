{
    "logger": {
        "colorize": true,
        "morganFormat": "dev",
        "json": false,
        "level": "info",
        "timestamp": true
    },
    "requestSizeLimit": "1gb",
    "storageProviders": [
        {
            "type": "git",
            "url": "http://gitrest:3000",
            "name": "git",
            "isDefault": true
        },
        {
            "type": "cobalt",
            "url": "http://cobalt",
            "name": "cobalt",
            "isDefault": false
        }
    ],
    "redis": {
        "host": "redis",
        "port": 6379
    }
}