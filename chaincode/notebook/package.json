{
  "name": "@chaincode/notebook",
  "version": "0.1.2",
  "description": "Notebook that takes in items and appends them to a managed shared string, map, and poll",
  "main": "server.js",
  "author": "Microsoft",
  "scripts": {
    "build": "npm run tslint && npm run webpack",
    "start": "webpack-serve webpack.config.js --open",
    "tslint": "tslint 'src/**/*.ts'",
    "webpack": "webpack --config webpack.config.js --open",
    "update": "npm run build && npm version patch && npm publish --registry http://localhost:4873"
  },
  "dependencies": {
    "@prague/client-api": "^0.1.3556",
    "@prague/loader": "^0.1.3556",
    "@prague/loader-web": "^0.1.3556",
    "@prague/runtime-definitions": "^0.1.3556",
    "@prague/socket-storage": "^0.1.3556",
    "@types/debug": "0.0.30",
    "@types/google-maps": "^3.2.0",
    "@types/node": "^10.9.4",
    "axios": "^0.18.0",
    "debug": "^4.0.1",
    "google-maps": "^3.3.0",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  }
}
