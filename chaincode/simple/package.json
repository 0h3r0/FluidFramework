{
  "name": "@chaincode/simple",
  "version": "0.0.14",
  "description": "Simplest example of chaincode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run tsc && npm run webpack && npm run tslint",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js",
    "tslint": "tslint 'src/**/*.ts'",
    "start": "webpack-serve webpack.config.js --port 8081",
    "update": "npm run build && npm version patch && npm publish --registry http://localhost:4873"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@types/uuid": "^3.4.4",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.1.4493",
    "@prague/map": "^0.1.4493",
    "@prague/runtime-definitions": "^0.1.4493",
    "uuid": "^3.3.2"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  }
}
