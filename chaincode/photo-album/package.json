  "devDependencies": {
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.1.1",
    "webpack-serve": "^2.0.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.1.0",
    "@prague/client-ui": "^0.1.3432",
    "@prague/map": "^0.1.0",
    "@prague/runtime-definitions": "^0.1.3328",
    "@prague/shared-string": "^0.1.0",
    "@prague/stream": "^0.1.0",
    "@types/node": "^10.5.8",
    "css-loader": "^1.0.0",
    "style-loader": "^0.23.0"
