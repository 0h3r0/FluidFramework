{
  "name": "@chaincode/photo-album",
  "version": "0.1.76",
  "description": "Photo album",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run tsc && npm run webpack && npm run tslint",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js",
