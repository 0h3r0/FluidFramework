{
  "name": "@prague/app-datastore",
  "version": "0.8.0",
  "description": "Prague app model data store",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@prague/component-core-interfaces": "^0.8.0",
    "@prague/container-definitions": "^0.8.0",
    "@prague/container-loader": "^0.8.0",
    "@prague/loader-web": "^0.8.0",
    "@prague/protocol-definitions": "^0.8.0",
    "@prague/routerlicious-host": "^0.8.0",
    "@prague/routerlicious-socket-storage": "^0.8.0",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.4.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.1.6",
    "@prague/build-common": "^0.8.0",
    "@types/debug": "^0.0.31",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^10.0.1",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "license": "MIT"
}
