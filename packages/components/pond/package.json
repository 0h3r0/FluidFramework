{
  "name": "@chaincode/pond",
  "description": "Grab bag of fluid scenarios.",
  "version": "0.7.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "start": "prague-dev-server --config webpack.config.js --live --package package.json",
    "deploy": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\""
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "@prague/aqueduct": "^0.7.0",
    "@prague/build-common": "^0.7.0",
    "@prague/component-runtime": "^0.7.0",
    "@prague/container-definitions": "^0.7.0",
    "@prague/framework-definitions": "^0.7.0",
    "@prague/map": "^0.7.0",
    "@prague/runtime-definitions": "^0.7.0",
    "@prague/shared-object-common": "^0.7.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@prague/prague-dev-server": "~3.6.0",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.11",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
