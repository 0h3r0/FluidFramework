{
  "name": "@component/collection-components",
  "description": "Progress bar collection",
  "version": "0.4.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "devDependencies": {
    "@prague/build-common": "^0.4.0",
    "typescript": "~3.4.5"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@prague/app-component": "^0.4.0",
    "@prague/component-runtime": "^0.4.0",
    "@prague/container-definitions": "^0.4.0",
    "@prague/runtime-definitions": "^0.4.0",
    "@prague/shared-object-common": "^0.4.0",
    "@prague/map": "^0.4.0",
    "@types/node": "^10.12.12",
    "bootstrap": "^3.3.7"
  }
}
