{
  "name": "@prague/app-component",
  "version": "0.3.0",
  "description": "Chaincodeable component base class",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "concurrently": "^4.1.0",
    "@types/debug": "^0.0.31",
    "@types/node": "^10.12.12",
    "@types/uuid": "^3.4.4",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.3.0",
    "@prague/component-runtime": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/cell": "^0.3.0",
    "@prague/map": "^0.3.0",
    "@prague/container-runtime": "^0.3.0",
    "@prague/runtime-definitions": "^0.3.0",
    "@prague/sequence": "^0.3.0",
    "@prague/stream": "^0.3.0",
    "debug": "^3.1.0",
    "uuid": "^3.3.2"
  }
}
