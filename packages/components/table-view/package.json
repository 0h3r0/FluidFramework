{
  "name": "@chaincode/table-view",
  "version": "0.3.0",
  "description": "Chaincode component that provides a view for a table-document.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/ && copyfiles -u 1 \"src/**/*.css\" lib/",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --env=\"production\"",
    "webpack:full": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\"",
    "dev": "webpack --env=\"development\" --watch",
    "publish-local": "npm publish --registry http://localhost:4873",
    "publish-wu2": "npm run build && npm publish --registry https://packages.wu2.prague.office-int.com",
    "publish-patch-local": "npm version patch && npm run publish-local"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@chaincode/table-document": "^0.3.0",
    "@prague/app-component": "^0.3.0",
    "@prague/app-datastore": "^0.3.0",
    "@prague/component-runtime": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/flow-util": "^0.3.0",
    "@prague/map": "^0.3.0",
    "@prague/merge-tree": "^0.3.0",
    "@prague/container-runtime": "^0.3.0",
    "@prague/runtime-definitions": "^0.3.0",
    "@prague/utils": "^0.3.0"
  },
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",    
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "typescript": "^3.4.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
