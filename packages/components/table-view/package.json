{
  "name": "@chaincode/table-view",
  "version": "0.10.0",
  "description": "Chaincode component that provides a view for a table-document.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/ && copyfiles -u 1 \"src/**/*.css\" lib/",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@chaincode/table-document": "^0.10.0",
    "@prague/aqueduct": "^0.10.0",
    "@prague/component-core-interfaces": "^0.10.0",
    "@prague/flow-util": "^0.10.0",
    "@prague/map": "^0.10.0",
    "@prague/runtime-definitions": "^0.10.0"
  },
  "devDependencies": {
    "@prague/build-common": "^0.10.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "license": "MIT"
}
