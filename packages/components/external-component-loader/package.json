{
  "name": "@chaincode/externalcomponentloader",
  "description": "Loads external components based on their url",
  "version": "0.8.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:tsc npm:build:esnext npm:tslint",
    "build:compile": "npm run build",
    "build:full": "npm run build",
    "build:full:compile": "npm run build",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\"",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "start": "prague-dev-server --config webpack.config.js --live --package package.json",
    "deploy": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "devDependencies": {
    "@prague/build-common": "^0.8.0",
    "@prague/prague-dev-server": "~3.6.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "dependencies": {
    "@prague/aqueduct": "^0.8.0",
    "@prague/component-core-interfaces": "^0.8.0",
    "@prague/component-runtime": "^0.8.0",
    "@prague/container-definitions": "^0.8.0",
    "@prague/container-runtime": "^0.8.0",
    "@prague/loader-web": "^0.8.0",
    "@prague/map": "^0.8.0",
    "@prague/merge-tree": "^0.8.0",
    "@prague/runtime-definitions": "^0.8.0",
    "@prague/sequence": "^0.8.0",
    "@prague/shared-object-common": "^0.8.0",
    "@prague/utils": "^0.8.0",
    "uuid": "^3.3.2"
  },
  "license": "MIT"
}
