<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{ title }}</title>
</head>
<body class="{{ optionalBodyClass }}">
    <div id="content"></div>

    <script src="{{#hfurl}}/public/scripts/dist/loader.js{{/hfurl}}"></script>
    {{#scripts}}
        {{#scripts}}
        <script id="{{id}}" src="{{url}}" async></script>
        {{/scripts}}
    {{/scripts}}

    <script>
        var cache = {{{ cache }}};
        var timings = "{{ timings }}";
        {{#chaincode}}
        var chaincode = {{{ chaincode }}};
        {{/chaincode}}
        {{^chaincode}}
        var chaincode;
        {{/chaincode}}
        var url = "{{ url }}";
        var resolved = {{{ resolved }}};
        var jwt = "{{ jwt }}";
        var npm = "{{ npm }}";
        var config = {{{ config }}};

        {{#scripts}}
        var scriptIds = [
            {{#scripts}}
            "{{id}}",
            {{/scripts}}
        ];
        {{/scripts}}
        {{^scripts}}
        var scriptIds = [];
        {{/scripts}}
  
        loader.initialize(
            window.location.href,
            resolved,
            cache,
            chaincode,
            scriptIds,
            npm,
            jwt,
            config);
    </script>
</body>
</html>
