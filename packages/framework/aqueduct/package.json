{
  "name": "@prague/aqueduct",
  "version": "0.5.0",
  "description": "A set of implementations for fluid framework interfaces.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@prague/container-definitions": "^0.5.0",
    "@prague/container-runtime": "^0.5.0",
    "@prague/map": "^0.5.0",
    "@prague/runtime-definitions": "^0.5.0"
  },
  "devDependencies": {
    "@prague/build-common": "^0.5.0",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "~3.4.5"
  },
  "license": "MIT"
}
