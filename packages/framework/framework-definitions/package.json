  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {},
  "devDependencies": {
    "@prague/build-common": "^0.4.0",
    "concurrently": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "~3.4.5"
  }
}
