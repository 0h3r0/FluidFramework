{
  "name": "@prague/framework-definitions",
  "version": "0.4.0",
  "description": "Definitions for fluid framework.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
