{
  "name": "@prague/sequence",
  "version": "0.3.0",
  "description": "Distributed map",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "test": "mocha --recursive dist/test/**/*.spec.js",
    "testfarm": "node dist/test/testFarm.js",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "test:newsnapfiles": "node dist/test/createSnapshotFiles.js"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "concurrently": "^4.1.0",
    "@prague/gitresources": "^0.3.0",
    "@prague/services-client": "^0.3.0",
    "@types/diff": "^3.5.1",
    "@types/random-js": "^1.0.31",
    "@types/xmldoc": "^1.1.3",
    "diff": "^3.5.0",
    "random-js": "^1.0.8",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.4.0",
    "xmldoc": "^1.1.2",
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.0.1"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/map": "^0.3.0",
    "@prague/merge-tree": "^0.3.0",
    "@prague/runtime-definitions": "^0.3.0",
    "@prague/utils": "^0.3.0",
    "@types/uuid": "^3.4.4",
    "lodash": "^4.17.11",
    "uuid": "^3.3.2"
  }
}
