{
  "name": "@prague/client-api",
  "version": "0.9.0",
  "description": "Prague client API",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "dependencies": {
    "@prague/cell": "^0.9.0",
    "@prague/component-core-interfaces": "^0.9.0",
    "@prague/component-runtime": "^0.9.0",
    "@prague/consensus-ordered-collection": "^0.9.0",
    "@prague/consensus-register-collection": "^0.9.0",
    "@prague/container-definitions": "^0.9.0",
    "@prague/container-loader": "^0.9.0",
    "@prague/container-runtime": "^0.9.0",
    "@prague/gitresources": "^0.9.0",
    "@prague/map": "^0.9.0",
    "@prague/merge-tree": "^0.9.0",
    "@prague/protocol-definitions": "^0.9.0",
    "@prague/runtime-definitions": "^0.9.0",
    "@prague/sequence": "^0.9.0",
    "@prague/shared-object-common": "^0.9.0",
    "@prague/stream": "^0.9.0",
    "@prague/utils": "^0.9.0",
    "@types/async": "^2.0.50",
    "@types/node": "^10.12.12",
    "@types/random-js": "^1.0.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.4",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "debug": "^4.1.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "performance-now": "^2.1.0",
    "random-js": "^1.0.8",
    "sha.js": "^2.4.11",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.1.6",
    "@prague/build-common": "^0.9.0",
    "@types/debug": "^0.0.31",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.5",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "xmldoc": "^1.1.2"
  },
  "license": "MIT"
}
