{
  "name": "@prague/socket-storage-shared",
  "version": "0.5.0",
  "description": "Shared package across different socket-storage implementation",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@prague/container-definitions": "^0.5.0",
    "@prague/utils": "^0.5.0",
    "debug": "^4.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@prague/build-common": "^0.5.0",
    "@types/debug": "^0.0.31",
    "@types/node": "^10.12.12",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "~3.4.5"
  }
}
