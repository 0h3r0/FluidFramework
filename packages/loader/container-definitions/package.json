{
  "name": "@prague/container-definitions",
  "version": "0.3.0",
  "description": "Prague container definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "npm run tsc",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.4.0"
  },
  "dependencies": {
    "@types/node": "^10.12.12"
  }
}
