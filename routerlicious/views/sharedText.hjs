{{<layout}}
    {{$styles}}
        <link href="{{#hfurl}}/public/stylesheets/dist/style.css{{/hfurl}}" rel="stylesheet" />
    {{/styles}}

    {{$content}}
    {{/content}}

    {{$scripts}}
        <script src="{{#hfurl}}/public/scripts/dist/index.js{{/hfurl}}"></script>
        <script>
            var config = {{{ config }}};
            var version = {{{ version }}};
            var options = {{{ options }}};
            var connect = {{{ connect }}};
            console.log(`Starting load at - ${performance.now()}`);
            controller.sharedText.load(
                "{{ id }}",
                version,
                {{ pageInk }},
                {{ disableCache }},
                config,
                "{{ template }}",
                connect,
                options);
        </script>
    {{/scripts}}
{{/layout}}