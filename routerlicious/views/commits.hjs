{{<layout}}
    {{$styles}}
        <link href="{{#hfurl}}/public/stylesheets/dist/style.css{{/hfurl}}" rel="stylesheet" />
        <link href="//cdnjs.cloudflare.com/ajax/libs/gitgraph.js/1.11.4/gitgraph.css" rel="stylesheet" />
    {{/styles}}

    {{$content}}
        <div class="container-fluid">
            <div id="commitsView"></div>
            <canvas id="gitGraph" class="graph-canvas"></canvas>
        </div>
    {{/content}}

    {{$scripts}}
        <script src="{{#hfurl}}/public/scripts/dist/index.js{{/hfurl}}"></script>
        <script>
            var versions = {{{ versions }}};
            controller.commits.load("{{ type }}","{{ id }}", versions);
        </script>
    {{/scripts}}
{{/layout}}