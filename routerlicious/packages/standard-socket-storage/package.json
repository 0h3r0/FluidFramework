{
  "name": "@prague/standard-socket-storage",
  "version": "0.1.0",
  "description": "Standardized socket storage implementation",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "npm run tsc && npm run build:esnext && npm run tslint",
    "build:full": "npm run build",
    "tsc": "tsc",
    "build:esnext": "tsc --outDir lib --m esnext",
    "test": "mocha --recursive dist/test",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@prague/client-api": "^0.1.0",
    "@prague/gitresources": "^0.1.0",
    "@prague/runtime-definitions": "^0.1.0",
    "@prague/services-client": "^0.1.0",
    "@prague/socket-storage-shared": "^0.1.0",
    "axios": "^0.18.0",
    "socket.io-client": "^2.1.1"
  }
}
