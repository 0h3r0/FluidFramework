{
  "name": "@prague/client-ui",
  "version": "0.1.0",
  "description": "Prague client UI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run tsc && npm run tslint",
    "build:full": "npm run build",
    "clean": "rimraf dist",
    "test": "mocha --recursive dist/test",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "devDependencies": {
    "@types/jsdom": "^11.12.0",
    "@types/mocha": "^5.2.5",
    "jsdom": "^12.0.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.1.0",
    "@prague/app-ui": "^0.1.0",
    "@prague/cell": "^0.1.0",
    "@prague/client-api": "^0.1.0",
    "@prague/loader": "^0.1.0",
    "@prague/loader-web": "^0.1.0",
    "@prague/map": "^0.1.0",
    "@prague/merge-tree": "^0.1.0",
    "@prague/merge-tree-utils": "^0.1.0",
    "@prague/runtime-definitions": "^0.1.0",
    "@prague/shared-string": "^0.1.0",
    "@prague/socket-storage": "^0.1.0",
    "@prague/stream": "^0.1.0",
    "@prague/utils": "^0.1.0",
    "@types/d3": "^5.0.0",
    "@types/debug": "^0.0.30",
    "@types/jquery": "^3.3.6",
    "@types/node": "^10.9.4",
    "@types/node-geocoder": "^3.19.0",
    "d3": "^5.7.0",
    "debug": "^3.1.0",
    "geocoder": "^0.2.3",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "node-geocoder": "^3.22.0",
    "parse-address": "^1.1.0",
    "performance-now": "^2.1.0",
    "shape-detector": "^0.2.1",
    "webcola": "^3.3.8"
  }
}
