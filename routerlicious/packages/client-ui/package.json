{
  "name": "@prague/client-ui",
  "version": "0.1.0",
  "description": "Server to assign sequence numbers and route deltas between connected clients",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run tsc && npm run tslint",
    "build:full": "npm run build && npm run build:esnext",
    "build:esnext": "tsc --outDir lib --m esnext",
    "test": "mocha --recursive dist/test",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tsc": "tsc",
    "tslint": "tslint 'src/**/*.ts'",
    "beast": "npm test -- --beast --grep beastTest"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "devDependencies": {
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@prague/client-api": "^0.1.0",
    "@types/node": "^10.9.4"
  }
}
