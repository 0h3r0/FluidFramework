{
  "name": "@chaincode/flow-host",
  "description": "React component that hosts a Flow-Editor instance w/toolbar, etc.",
  "version": "0.2.3",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/main.bundle.js",
  "module": "dist/main.bundle.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:compile",
    "build:compile": "npm run webpack:dev",
    "build:dev": "npm run webpack:dev",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo",
    "start": "prague-dev-server --config webpack.config.js --package package.json --env=\"developement\"",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"developement\"",
    "webpack:full": "webpack --env=\"production\"",
    "publish-local": "npm publish --registry http://localhost:4873",
    "publish-wu2": "npm run build && npm version patch && npm publish --registry https://packages.wu2.prague.office-int.com",
    "republish": "docker-compose stop verdaccio && docker-compose rm -f -v verdaccio && docker-compose create --force-recreate verdaccio && docker-compose start verdaccio && npm run publish-local"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@chaincode/chart-view": "^0.3.0",
    "@chaincode/flow-document": "^0.3.0",
    "@chaincode/flow-editor": "^0.3.0",
    "@chaincode/table-document": "^0.3.0",
    "@chaincode/table-view": "^0.3.0",
    "@prague/app-component": "^0.3.0",
    "@prague/app-datastore": "^0.3.0",
    "@prague/component": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/flow-util": "^0.3.0",
    "@prague/runtime": "^0.3.0",
    "@prague/runtime-definitions": "^0.3.0",
    "@prague/utils": "^0.3.0",
    "@uifabric/icons": "~6.3.0",
    "office-ui-fabric-react": "^6.108.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "@prague/prague-dev-server": "^3.3.3",
    "concurrently": "^4.1.0",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.11",
    "css-loader": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typings-for-css-modules-loader": "^1.7.0",
    "typescript": "^3.4.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
