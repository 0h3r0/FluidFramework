{
  "name": "@chaincode/table-slice",
  "version": "0.2.2",
  "description": "Chaincode component that provides a slice of table-document.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "scripts": {
    "build": "npm run tsc && npm run build:esnext && npm run tslint",
    "build:esnext": "tsc --outDir lib --m esnext",
    "build:full": "npm run build",
    "clean": "rimraf dist && rimraf lib",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\"",
    "webpack:full": "npm run webpack",
    "publish-local": "npm run webpack && npm publish --registry http://localhost:4873",
    "publish-patch-local": "npm version patch && npm run publish-local",
    "publish-wu2": "npm run webpack && npm publish --registry https://packages.wu2.prague.office-int.com"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@chaincode/table-document": "^0.2.0",
    "@prague/app-component": "^0.2.0",
    "@prague/app-datastore": "^0.2.0",
    "@prague/component": "^0.2.0",
    "@prague/container-definitions": "^0.2.0",
    "@prague/flow-util": "^0.2.0",
    "@prague/map": "^0.2.0",
    "@prague/merge-tree": "^0.2.0",
    "@prague/runtime": "^0.2.0",
    "@prague/runtime-definitions": "^0.2.0",
    "@prague/utils": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
