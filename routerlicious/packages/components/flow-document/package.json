{
  "name": "@chaincode/flow-document",
  "description": "Experimental flow editor for shared journaling.",
  "version": "0.2.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist && rimraf lib",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --env=\"production\"",
    "webpack:full": "webpack --env=\"production\"",
    "dev": "webpack --env=\"development\" --watch",
    "publish-local": "npm publish --registry http://localhost:4873",
    "publish-patch-local": "npm version patch && npm run publish-local",
    "publish-wu2": "npm run build && npm publish --registry https://packages.wu2.prague.office-int.com"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@prague/app-component": "^0.2.0",
    "@prague/app-datastore": "^0.2.0",
    "@prague/component": "^0.2.0",
    "@prague/container-definitions": "^0.2.0",
    "@prague/map": "^0.2.0",
    "@prague/merge-tree": "^0.2.0",
    "@prague/runtime": "^0.2.0",
    "@prague/runtime-definitions": "^0.2.0",
    "@prague/sequence": "^0.2.0",
    "@prague/utils": "^0.2.0"
  },
  "devDependencies": {
    "@prague/build-common": "^0.2.0",
    "concurrently": "^4.1.0",
    "@types/debug": "^0.0.31",
    "@types/node": "^10.12.12",
    "debug": "^3.1.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.4.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  }
}
