<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{ title }}</title>
</head>
<body class="{{ optionalBodyClass }}">
    <div id="content"></div>

    <script src="{{#hfurl}}/public/scripts/dist/loader.js{{/hfurl}}"></script>
    {{#pkg}}
        {{#scripts}}
        <script src="{{.}}"></script>
        {{/scripts}}
    {{/pkg}}

    <script>
        var config = {{{ config }}};
        var cache = {{{ cache }}};

        var token = "{{ token }}";
        var chaincode = "{{ chaincode }}";
        var tenantId = "{{ tenantId }}";
        var documentId = "{{ documentId }}";
        var path = "{{ path }}";

        {{#pkg}}
        var entry = "{{ entrypoint }}";
        {{/pkg}}
        {{^repo}}
        var entry = null;
        {{/repo}}
  
        loader.initialize(
            tenantId,
            documentId,
            path,
            cache,
            token,
            config,
            chaincode,
            entry,
            config.npm);
    </script>
</body>
</html>
