{
  "name": "@prague/client-api",
  "version": "0.2.0",
  "description": "Prague client API",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run tsc && npm run build:esnext && npm run tslint",
    "build:full": "npm run build",
    "clean": "rimraf dist && rimraf lib",
    "test": "mocha --recursive dist/test",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tsc": "tsc",
    "build:esnext": "tsc --outDir lib --m esnext",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "dependencies": {
    "@prague/api-definitions": "^0.2.0",
    "@prague/cell": "^0.2.0",
    "@prague/container-definitions": "^0.2.0",
    "@prague/gitresources": "^0.2.0",
    "@prague/loader": "^0.2.0",
    "@prague/map": "^0.2.0",
    "@prague/merge-tree": "^0.2.0",
    "@prague/runtime-definitions": "^0.2.0",
    "@prague/sequence": "^0.2.0",
    "@prague/stream": "^0.2.0",
    "@prague/utils": "^0.2.0",
    "@types/async": "^2.0.50",
    "@types/node": "^10.12.12",
    "@types/random-js": "^1.0.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.4",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "performance-now": "^2.1.0",
    "random-js": "^1.0.8",
    "sha.js": "^2.4.11",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2",
    "xmldoc": "^1.1.2"
  }
}
