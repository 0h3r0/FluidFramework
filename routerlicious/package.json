{
  "name": "routerlicious",
  "version": "0.1.0",
  "description": "Server to assign sequence numbers and route deltas between connected clients",
  "main": "index.js",
  "scripts": {
    "browserify": "mkdirp public/scripts/dist && concurrently \"npm run browserify:canvas\" \"npm run browserify:maps\" \"npm run browserify:cell\" \"npm run browserify:sharedText\" \"npm run browserify:scribe\" \"npm run browserify:perf\"",
    "browserify:canvas": "browserify src/alfred/controllers/canvas.ts -o public/scripts/dist/canvas.js -s controller -p [ tsify -t es5 ]",
    "browserify:maps": "browserify src/alfred/controllers/maps.ts -o public/scripts/dist/maps.js -s controller -p [ tsify -t es5 ]",
    "browserify:cell": "browserify src/alfred/controllers/cell.ts -o public/scripts/dist/cell.js -s controller -p [ tsify -t es5 ]",
    "browserify:sharedText": "browserify src/alfred/controllers/sharedText.ts -o public/scripts/dist/sharedText.js -s controller -p [ tsify -t es5 ]",
    "browserify:scribe": "browserify src/alfred/controllers/scribe.ts -o public/scripts/dist/scribe.js --debug -s controller -p [ tsify -t es5 ]",
    "browserify:perf": "browserify src/alfred/controllers/perf.ts -o public/scripts/dist/perf.js --debug -s controller -p [ tsify -t es5 ]",
    "build": "concurrently \"npm run tslint\" \"npm run tsc\" \"npm run browserify\" \"npm run less\"",
    "build:watch": "watch 'npm run tsc && npm run browserify' src",
    "less": "lessc src/stylesheets/style.less public/stylesheets/dist/style.css",
    "alfred": "node dist/alfred/www.js",
    "alfred:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/alfred/www.js -w dist",
    "alfred:debug:windows": "nodemon --exitcrash -L --debug=0.0.0.0:5858 dist/alfred/www.js -w dist",
    "alfred:watch": "nodemon --exitcrash dist/alfred/www.js -w dist",
    "deli": "node dist/deli/index.js",
    "deli:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/deli/index.js -w dist",
    "deli:debug:windows": "nodemon --exitcrash -L --debug=0.0.0.0:5858 dist/deli/index.js -w dist",
    "deli:watch": "nodemon --exitcrash dist/deli/index.js -w dist",
    "paparazzi": "node dist/paparazzi/index.js",
    "paparazzi:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/paparazzi/index.js -w dist",
    "paparazzi:debug:windows": "nodemon --exitcrash -L --debug=0.0.0.0:5858 dist/paparazzi/index.js -w dist",
    "paparazzi:watch": "nodemon --exitcrash dist/paparazzi/index.js -w dist",
    "scriptorium": "node dist/scriptorium/index.js",
    "scriptorium:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/scriptorium/index.js -w dist",
    "scriptorium:debug:windows": "nodemon --exitcrash -L --debug=0.0.0.0:5858 dist/scriptorium/index.js -w dist",
    "scriptorium:watch": "nodemon --exitcrash dist/scriptorium/index.js -w dist",
    "tmz": "node dist/tmz/index.js",
    "tmz:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/tmz/index.js",
    "tmz:debug:windows": "nodemon --exitcrash -L --debug=0.0.0.0:5858 dist/tmz/index.js",
    "tmz:watch": "nodemon --exitcrash dist/tmz/index.js -w dist",
    "start": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.debug.yml up",
    "start:debug:windows": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.debug.windows.yml up",
    "test": "nyc -x dist/test/**/* mocha dist/test",
    "tsc": "tsc",
    "tslint": "tslint 'src/**/*.ts'",
    "beast": "node dist/merge-tree/beastTest.js",
    "perf:deli": "node dist/perf/deli.js",
    "perf:deli:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/perf/deli.js -w dist",
    "pretest:perf:deli": "docker-compose -f docker-compose.test.deli.yml down --remove-orphans",
    "test:perf:deli": "npm run pretest:perf:deli; docker-compose -f docker-compose.test.deli.yml up",
    "perf:scriptorium": "node dist/perf/scriptorium.js",
    "perf:scriptorium:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/perf/scriptorium.js -w dist",
    "pretest:perf:scriptorium": "docker-compose -f docker-compose.test.scriptorium.yml down --remove-orphans",
    "test:perf:scriptorium": "npm run pretest:perf:scriptorium; docker-compose -f docker-compose.test.scriptorium.yml up",
    "perf:alfred": "node dist/perf/alfred.js",
    "perf:alfred:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/perf/alfred.js -w dist",
    "pretest:perf:alfred": "docker-compose -f docker-compose.test.alfred.yml down --remove-orphans",
    "test:perf:alfred": "npm run pretest:perf:alfred; docker-compose -f docker-compose.test.alfred.yml up",
    "perf:client": "node dist/perf/client.js",
    "perf:client:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/perf/client.js -w dist",
    "pretest:perf:client": "docker-compose -f docker-compose.test.client.yml down --remove-orphans",
    "test:perf:client": "npm run pretest:perf:client; docker-compose -f docker-compose.test.client.yml up",
    "perf:kafka:producer": "node dist/perf/kafkaproducer.js",
    "perf:kafka:producer:debug": "nodemon --exitcrash --debug=0.0.0.0:5858 dist/perf/kafkaproducer.js -w dist",
    "perf:kafka:consumer": "node dist/perf/kafkaconsumer.js",
    "perf:kafka:consumer:debug": "nodemon --exitcrash --debug=0.0.0.0:5859 dist/perf/kafkaconsumer.js -w dist",
    "perf:kafka": "concurrently \"npm run perf:kafka:producer\" \"npm run perf:kafka:consumer\"",
    "perf:kafka:debug": "concurrently \"npm run perf:kafka:producer:debug\" \"npm run perf:kafka:consumer:debug\"",
    "pretest:perf:kafka": "docker-compose -f docker-compose.test.kafka.yml down --remove-orphans",
    "test:perf:kafka": "npm run pretest:perf:kafka; docker-compose -f docker-compose.test.kafka.yml up"
  },
  "author": "Microsoft",
  "license": "Apache-2.0",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@types/amqplib": "^0.5.1",
    "@types/async": "^2.0.40",
    "@types/commander": "^2.9.0",
    "@types/debug": "0.0.29",
    "@types/diff": "^3.2.0",
    "@types/jquery": "^2.0.41",
    "@types/kafka-node": "^1.3.2",
    "@types/lodash": "^4.14.61",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.1.42",
    "@types/morgan": "^1.7.32",
    "@types/mqtt": "0.0.34",
    "@types/nconf": "0.0.34",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.3.3",
    "@types/progress": "^2.0.0",
    "@types/random-js": "^1.0.28",
    "@types/request": "0.0.42",
    "@types/serve-favicon": "^2.2.28",
    "@types/socket.io": "^1.4.28",
    "@types/socket.io-client": "^1.4.29",
    "@types/socket.io-redis": "^1.0.21",
    "@types/split": "^0.3.28",
    "@types/winston": "^2.3.3",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "concurrently": "^3.4.0",
    "kafka-rest": "0.0.5",
    "less": "^2.7.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "tsify": "^3.0.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "amqp10": "^3.0.0",
    "amqplib": "^0.5.1",
    "async": "^2.4.1",
    "azure-event-hubs": "0.0.7",
    "azure-iot-device": "^1.1.12",
    "azure-iot-device-mqtt": "^1.1.12",
    "azure-iothub": "^1.1.11",
    "azure-sb": "^0.10.4",
    "azure-storage": "^2.1.0",
    "body-parser": "^1.17.1",
    "commander": "^2.9.0",
    "debug": "^2.6.8",
    "diff": "^3.2.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.2",
    "hjs": "0.0.6",
    "jquery": "^3.2.1",
    "kafka-node": "^1.6.0",
    "lodash": "^4.17.4",
    "minio": "^3.1.3",
    "mongodb": "^2.2.25",
    "moniker": "^0.1.2",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "node-uuid": "^1.4.8",
    "passport": "^0.3.2",
    "progress": "^2.0.0",
    "random-js": "^1.0.8",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "resize-observer-polyfill": "^1.4.2",
    "serve-favicon": "^2.4.1",
    "socket.io": "^1.7.3",
    "socket.io-emitter": "^2.0.0",
    "socket.io-redis": "^4.0.0",
    "split": "^1.0.0",
    "static-expiry": "0.0.11",
    "winston": "^2.3.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "socket.io-client": "global:io"
  }
}
