{
  "name": "routerlicious",
  "version": "0.1.0",
  "description": "Server to assign sequence numbers and route deltas between connected clients",
  "main": "index.js",
  "scripts": {
    "browserify": "mkdirp public/scripts/dist && browserify dist/server/controllers/home.js -o public/scripts/dist/home.js -s controller",
    "build": "npm run tsc && npm run tslint && npm run browserify && npm run less",
    "build:watch": "watch 'npm run tsc && npm run browserify' src",
    "less": "lessc src/stylesheets/style.less public/stylesheets/dist/style.css",
    "nodemon": "nodemon dist/server/www.js -w dist",
    "deli": "nodemon dist/deli/index.js -w dist",
    "paparazzi": "nodemon dist/paparazzi/index.js -w dist",
    "scriptorium": "nodemon dist/scriptorium/index.js -w dist",
    "tmz": "nodemon dist/tmz/index.js -w dist",
    "start": "node dist/server/www.js",
    "test": "nyc -x dist/test/**/* mocha dist/test",
    "tsc": "tsc",
    "tslint": "tslint 'src/**/*.ts'"
  },
  "author": "Microsoft",
  "license": "Apache-2.0",
  "repository": "microsoft/prague",
  "devDependencies": {
    "@types/jquery": "^2.0.41",
    "@types/lodash": "^4.14.61",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.1.42",
    "@types/morgan": "^1.7.32",
    "@types/nconf": "0.0.34",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.3.3",
    "@types/serve-favicon": "^2.2.28",
    "@types/socket.io": "^1.4.28",
    "@types/socket.io-client": "^1.4.29",
    "@types/socket.io-redis": "^1.0.21",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "less": "^2.7.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "amqp10": "^3.0.0",
    "azure-event-hubs": "0.0.7",
    "azure-sb": "^0.10.4",
    "azure-storage": "^2.1.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "hjs": "0.0.6",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.25",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "node-uuid": "^1.4.8",
    "passport": "^0.3.2",
    "redis": "^2.7.1",
    "serve-favicon": "^2.4.1",
    "socket.io": "^1.7.3",
    "socket.io-emitter": "^2.0.0",
    "socket.io-redis": "^4.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "socket.io-client": "global:io"
  }
}
