    "@types/amqplib": "^0.5.1",
    "@types/async": "^2.0.40",
    "@types/commander": "^2.9.0",
    "@types/compression": "0.0.33",
    "@types/debug": "0.0.29",
    "@types/diff": "^3.2.0",
    "@types/jquery": "^2.0.41",
    "@types/kafka-node": "^1.3.5",
    "@types/lodash": "^4.14.61",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.40",
    "@types/mongodb": "^2.1.42",
    "@types/morgan": "^1.7.32",
    "@types/mqtt": "0.0.34",
    "@types/nconf": "0.0.34",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.3.3",
    "@types/progress": "^2.0.0",
    "@types/random-js": "^1.0.28",
    "@types/redis": "^2.6.0",
    "@types/request": "0.0.45",
    "@types/serve-favicon": "^2.2.28",
    "@types/socket.io": "^1.4.28",
    "@types/socket.io-client": "^1.4.29",
    "@types/socket.io-redis": "^1.0.21",
    "@types/split": "^0.3.28",
    "@types/winston": "^2.3.3",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "concurrently": "^3.4.0",
    "kafka-rest": "0.0.5",
    "less": "^2.7.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "tsify": "^3.0.1",
    "tslint": "^4.5.1",
    "typescript": "2.3.x",
    "uglify-js": "^3.0.24",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "amqp10": "^3.0.0",
    "amqplib": "^0.5.1",
    "async": "^2.4.1",
    "azure-event-hubs": "0.0.7",
    "azure-iot-device": "^1.1.12",
    "azure-iot-device-mqtt": "^1.1.12",
    "azure-iothub": "^1.1.11",
    "azure-sb": "^0.10.4",
    "azure-storage": "^2.1.0",
    "body-parser": "^1.17.1",
    "commander": "^2.9.0",
    "compression": "^1.7.0",
    "crypto-js": "^3.1.9-1",
    "debug": "^2.6.8",
    "diff": "^3.2.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.2",
    "gitresources": "https://praguenpm.blob.core.windows.net/packages/gitresources-0.1.0.tgz",
    "hjs": "0.0.6",
    "jquery": "^3.2.1",
    "kafka-node": "^1.6.0",
    "lodash": "^4.17.4",
    "minio": "^3.1.3",
    "mkdirp": "^0.5.1",
    "mongodb": "^2.2.30",
    "moniker": "^0.1.2",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "node-uuid": "^1.4.8",
    "openpgp": "^2.5.8",
    "passport": "^0.3.2",
    "progress": "^2.0.0",
    "random-js": "^1.0.8",
    "redis": "^2.7.1",
