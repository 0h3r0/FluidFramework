{
    "logger": {
        "colorize": true,
        "morganFormat": "dev",
        "json": false,
        "level": "info",
        "timestamp": true,
        "label": "winston"
    },
    "mongo": {
        "endpoint": "mongodb://mongodb:27017",
        "collectionNames": {
            "deltas": "deltas",
            "documents": "documents",
            "partitions": "partitions"
        }
    },
    "kafka": {
        "lib": {
            "name": "kafka-node",
            "endpoint": "zookeeper:2181"
        }
    },
    "alfred": {
        "kafkaClientId": "alfred",
        "topic": "rawdeltas",
        "bucket": "snapshots",
        "restJsonSize": "50mb",
        "webSocketLib": "socket.io"
    },
    "deli": {
        "topics": {
            "receive": "rawdeltas",
            "send": "deltas"
        },
        "kafkaClientId": "deli",
        "groupId": "deli",
        "checkpointBatchSize": 10,
        "checkpointTimeIntervalMsec": 1000

    },
    "routemaster": {
        "topics": {
            "receive": "deltas",
            "send": "rawdeltas"
        },
        "clientId": "routemaster",
        "groupId": "routemaster",
        "checkpointBatchSize": 10,
        "checkpointTimeIntervalMsec": 1000
    },
    "paparazzi": {
        "queue": "snapshot",
        "bucket": "snapshots",
        "alfred": "http://alfred:3000",
        "tmz": "http://tmz:4000"
    },
    "scriptorium": {
        "topic": "deltas",
        "kafkaClientId": "scriptorium",
        "groupId": "scriptorium",
        "checkpointBatchSize": 1,
        "checkpointTimeIntervalMsec": 1000
    },
    "tmz": {
        "queue": "snapshot",
        "topic": "deltas",
        "kafkaClientId": "tmz",
        "groupId": "tmz",
        "port": 4000,
        "timeoutMSec": {
            "checker": 10000,
            "worker": 10000,
            "document": 600000
        },
        "tasks": {
            "spell": "server",
            "intel": "server",
            "snapshot": "both"
        },
        "workerType": "random",
        "workerAckTimeMSec": 250,
        "onlyServer": false
    },
    "redis": {
        "host": "redis",
        "port": 6379
    },
