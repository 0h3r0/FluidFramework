{
  "name": "headless-chrome",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tslint 'src/**/*.ts'",
    "build:prague": "webpack --color --config client-src/prague-loader/webpack.config.js",
    "build:full": "npm run build:prague && npm run build",
    "docker:build": "docker build --build-arg NPM_TOKEN=$(echo $NPM_TOKEN) -t headless-agent .",
    "docker:start": "docker run --rm -t --cap-add=SYS_ADMIN --network routerlicious_default headless-agent",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up"
  },
  "dependencies": {
    "@prague/container-definitions": "^0.2.5579",
    "@prague/container-loader": "^0.2.5579",
    "@prague/routerlicious-socket-storage": "^0.2.5579",
    "@prague/routerlicious-host": "^0.2.5579",
    "@prague/runtime-definitions": "^0.2.5579",
    "@prague/services": "^0.2.5579",
    "@prague/services-core": "^0.2.5579",
    "@prague/services-utils": "^0.2.5579",
    "@prague/utils": "^0.2.5579",
    "commander": "^2.15.1",
    "debug": "^3.1.0",
    "jsonwebtoken": "^8.5.0",
    "puppeteer": "^1.12.2",
    "redis": "^2.8.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/puppeteer": "^1.12.1",
    "@types/redis": "^2.8.10",
    "ts-loader": "^5.3.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
}
